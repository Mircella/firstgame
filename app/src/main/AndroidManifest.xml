<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ru.startandroid.firstgame">
    <uses-sdk android:minSdkVersion="17" android:targetSdkVersion="26"
        tools:overrideLibrary="android.support.v4, android.support.v7.appcompat, android.support.mediacompat, android.support.fragment,
        android.support.coreutils, android.support.annotation, android.support.constraint, android.support.graphics, android.support.coreui,
        android.support.graphics.drawable, android.support.compat, android.support.constraint.solver"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KrestikiNolikiActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>